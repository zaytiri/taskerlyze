<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" prefWidth="280.0"
         type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx/">
    <VBox alignment="CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
          AnchorPane.topAnchor="0.0" prefWidth="250.0"
          spacing="15">
        <Label text="Input new profile's name:"
               VBox.vgrow="ALWAYS"
               wrapText="true"
               textAlignment="JUSTIFY">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </Label>
        <TextField fx:id="profileName" VBox.vgrow="ALWAYS">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </TextField>
        <HBox alignment="CENTER" spacing="15.0">
            <Button fx:id="buttonAdd" mnemonicParsing="false"
                    text="Add Profile"/>
        </HBox>
    </VBox>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</fx:root>
