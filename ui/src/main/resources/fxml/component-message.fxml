<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<fx:root xmlns:fx="http://javafx.com/fxml"
         xmlns="http://javafx.com/javafx" type="javafx.scene.control.TitledPane" fx:id="messageTitledPane"
         stylesheets="@../css/achieved-view.css"
         expanded="false">
    <graphic>
        <Label id="name" text="Message"/>
    </graphic>
    <AnchorPane>
        <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <Label id="achieved-title" fx:id="messageType"/>
            <Label id="achieved-content" fx:id="messageContent"/>
        </VBox>
    </AnchorPane>
</fx:root>
